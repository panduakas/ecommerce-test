{
  "main": "./app/app.js",
  "version": "1.0.0",
  "private": false,
  "description": "ecommerce-test",
  "scripts": {
    "start": "node ./app/app.js --env=development",
    "start-pm2": "./node_modules/.bin/pm2 start ./app/app.js --name ecommerce-test --env=development",
    "start-prod": "./node_modules/.bin/pm2 start ./app/app.js --name ecommerce-test -i max --env=production",
    "delete-pm2": "./node_modules/.bin/pm2 delete ecommerce-test",
    "stop-pm2": "./node_modules/.bin/pm2 stop ./app/app.js --name ecommerce-test",
    "start-dev": "./node_modules/.bin/nodemon ./app/app.js",
    "test": "./node_modules/mocha/bin/mocha --exit --reporter spec --timeout 12000 --require ./app/app.js test/*.js",
    "eslint-fix": "./node_modules/.bin/eslint --fix .",
    "restart-pm2": "./node_modules/.bin/pm2 restart ./app/app.js --name ecommerce-test",
    "reload-pm2": "./node_modules/.bin/pm2 reload ./app/app.js --name ecommerce-test",
    "create-db": "node_modules/.bin/sequelize db:create --config=app/db/datasource/Mysql.js",
    "migrate": "node_modules/.bin/sequelize db:migrate --migrations-path=app/db/migrations --config=app/db/datasource/Mysql.js --debug",
    "seed": "./node_modules/.bin/sequelize db:seed:all --seeders-path=app/db/seeds --config=app/db/datasource/Mysql.js --debug"
  },
